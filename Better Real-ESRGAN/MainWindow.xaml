<Window x:Class="Better_Real_ESRGAN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Better_Real_ESRGAN"
        mc:Ignorable="d"
        Title="Better Real-ESRGAN" Height="450" Width="800">
    <Grid>
        <Grid>
            <Label Content="图片路径：" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
            <TextBox x:Name="ImagePathBox" HorizontalAlignment="Left" Margin="80,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="564"/>
            <Button x:Name="SelectFileBtn" Content="选择文件" HorizontalAlignment="Left" Margin="649,13,0,0" VerticalAlignment="Top" Click="SelectFileBtn_Click"/>
            <Button x:Name="SelectFolderBtn" Content="选择文件夹" HorizontalAlignment="Left" Margin="706,13,0,0" VerticalAlignment="Top" Click="SelectFolderBtn_Click"/>
        </Grid>

    </Grid>
</Window>
