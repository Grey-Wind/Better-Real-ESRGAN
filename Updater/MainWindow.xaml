<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Updater"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="MainWindow"
        mc:Ignorable="d"
        Title="Better Real-ESRGAN下载器"
        Height="200"
        Width="347"
        Background="#FFE0D6F6">
    <Grid>
        <Button Content="软件本体更新" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="100"/>
        <Button Content="下载模型" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="100"/>
        <Button Content="下载启动器" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Width="100"/>
        <Button Content="版权声明更新" HorizontalAlignment="Left" Margin="115,10,0,0" VerticalAlignment="Top" Width="100"/>
        <Button Content="动态链接库更新" HorizontalAlignment="Left" Margin="115,34,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="AllUpdateBtn" Content="一键更新下载" HorizontalAlignment="Left" Margin="115,58,0,0" VerticalAlignment="Top" Width="100" FontWeight="Bold" Background="#FFFC9F9F"/>
        <Label Content="建议使用一键更新&#xA;下载，省事而且简&#xA;单&#xA;这个更新器不更新" HorizontalAlignment="Left" Margin="220,10,0,0" VerticalAlignment="Top"/>
        <Label Content="当前安装版本：" HorizontalAlignment="Left" Margin="10,82,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="LocalVersion" HorizontalAlignment="Left" Margin="109,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"><Run Language="zh-cn" Text="检测版本中"/></TextBlock>
        <Label Content="GitHub版本：" HorizontalAlignment="Left" Margin="10,107,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="GitHubVersion" HorizontalAlignment="Left" Margin="109,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"><Run Language="zh-cn" Text="检测版本中"/></TextBlock>
        <Label Content="Gitee版本：" HorizontalAlignment="Left" Margin="10,132,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="GiteeVersion" HorizontalAlignment="Left" Margin="109,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"><Run Language="zh-cn" Text="检测版本中"/></TextBlock>
        <Label Content="版本检测是线上&#xD;&#xA;检测，网速不好&#xD;&#xA;可能会慢，正常&#xD;&#xA;现象" HorizontalAlignment="Left" Margin="220,84,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
